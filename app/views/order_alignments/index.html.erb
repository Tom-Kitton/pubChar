<p id="notice"><%= notice %></p>

<h1>Order Alignments</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_alignments.each do |order_alignment| %>
      <tr>
        <td><%= order_alignment.content %></td>
        <td><%= link_to 'Show', order_alignment %></td>
        <td><%= link_to 'Edit', edit_order_alignment_path(order_alignment) %></td>
        <td><%= link_to 'Destroy', order_alignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order Alignment', new_order_alignment_path %>
